<p id="notice"><%= notice %></p>

<h1>Frases da Dilma <3</h1>

<table>
  <thead>
    <tr>
      <th>Titulo</th>
      <th>Frase</th>
      <th>Data publicacao</th>
      <th>Data dilma</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @posts.each do |post| %>
      <tr>
        <td><%= post.titulo %></td>
        <td><%= post.frase %></td>
        <td><%= post.data_publicacao %></td>
        <td><%= post.data_dilma %></td>
        <td><%= link_to 'Show', post %></td>
        <td><%= link_to 'Edit', edit_post_path(post) %></td>
        <td><%= link_to 'Destroy', post, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Post', new_post_path %>
